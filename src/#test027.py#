# -*- coding: utf-8 -*-
from pygeocoder import Geocoder
import urllib.request

def download_pic(url,filename):
    img = urllib.request.urlopen(url)
    localfile = open( "./map/" + str(filename) + ".png" , 'wb')
    localfile.write(img.read())
    img.close()
    localfile.close()

address = '新宿　なか卯'
lat = "35.039352";
lng = "135.729265";
url = "http://maps.google.com/maps/api/staticmap?center=" + lat + "," + lng + "&zoom=13&size=6http://maps.google.com/maps/api/staticmap?center=35.039352,135.729265&zoom=13&size=180x120&markers=35.039352%2C135.729265&sensor=false&key=AIzaSyCuxTpu4wHcCz1M9S3GNLMfbCYmrc-b-dg40x120&markers=" + lat + "%2C" + lng + "&sensor=false&key=AIzaSyCuxTpu4wHcCz1M9S3GNLMfbCYmrc-b-dg";

print(url)

download_pic(url,address)
